<h1>Listing datafiles for Project: 
  <%= @project.name %>
</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @datafiles.each do |datafile| %>
  <tr>
    <td><%= datafile.name %></td>
    <td><%= datafile.status %></td>
    <td><%= link_to 'Show', project_datafile_path(@project.id, datafile.id) %></td>
  </tr>
<% end %>
</table>

<br />

